{
  "name": "the-library",
  "version": "1.0.0",
  "description": "A tool for searching the Google Books API",
  "main": "server.js",
  "jest": {
    "setupFiles": [
      "<rootDir>/testUtils/mock.js"
    ]
  },
  "test": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "dev": "webpack -w --mode development",
    "test": "NODE_ENV=test jest",
    "serve": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magentanova/the-library.git"
  },
  "author": "Justin Richards",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magentanova/the-library/issues"
  },
  "homepage": "https://github.com/magentanova/the-library#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  }
}
